; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Canvas",
"language": "GDScript",
"path": "res://Global/Classes/Canvas.gd"
}, {
"base": "Reference",
"class": "Element",
"language": "GDScript",
"path": "res://Global/Classes/Element.gd"
}, {
"base": "Reference",
"class": "Header",
"language": "GDScript",
"path": "res://Global/Classes/Header.gd"
}, {
"base": "Button",
"class": "TopButton",
"language": "GDScript",
"path": "res://Global/Classes/TopButton.gd"
} ]
_global_script_class_icons={
"Canvas": "",
"Element": "",
"Header": "",
"TopButton": ""
}

[application]

config/name="Scheduler"
run/main_scene="res://Main/Main.tscn"
config/icon="res://Assets/icon.png"

[autoload]

FileHandler="*res://Global/FileHandler.gd"
ThemeHandler="*res://Global/ThemeHandler.gd"
StyleConverter="*res://Global/StyleConverter.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/test_width=2048
window/size/test_height=1200
window/dpi/allow_hidpi=true

[importer_defaults]

texture={
"flags/filter": false,
"flags/repeat": 1
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
